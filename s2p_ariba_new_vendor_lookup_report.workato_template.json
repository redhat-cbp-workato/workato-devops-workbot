{
  "name": "[S2P] Ariba New Vendor Lookup Report",
  "schema": [
    {
      "label": "Record",
      "of": "object",
      "properties": [
        {
          "control_type": "text",
          "label": "Vendor ID",
          "type": "string",
          "name": "VendorID"
        },
        {
          "control_type": "text",
          "label": "Vendor name",
          "type": "string",
          "name": "VendorName",
          "optional": true
        },
        {
          "control_type": "text",
          "label": "Oracle ERP vendor ID",
          "type": "string",
          "name": "OracleERPVendorID",
          "optional": true
        },
        {
          "control_type": "date",
          "label": "JobTime",
          "type": "date_time",
          "name": "JobTime",
          "optional": false,
          "render_input": "date_conversion",
          "parse_output": "date_conversion"
        },
        {
          "name": "RecipeID",
          "type": "integer",
          "optional": false,
          "label": "RecipeID",
          "control_type": "integer",
          "parse_output": "integer_conversion",
          "render_input": "integer_conversion"
        },
        {
          "name": "SiteOU",
          "type": "integer",
          "optional": true,
          "label": "Site OU",
          "control_type": "integer",
          "parse_output": "integer_conversion",
          "render_input": "integer_conversion"
        },
        {
          "name": "SiteAddress",
          "type": "integer",
          "optional": true,
          "label": "Site Address",
          "control_type": "integer",
          "parse_output": "integer_conversion",
          "render_input": "integer_conversion"
        },
        {
          "name": "Bank",
          "type": "integer",
          "optional": true,
          "label": "Bank",
          "control_type": "integer",
          "parse_output": "integer_conversion",
          "render_input": "integer_conversion"
        },
        {
          "name": "INTBank",
          "type": "integer",
          "optional": true,
          "label": "INT Bank",
          "control_type": "integer",
          "parse_output": "integer_conversion",
          "render_input": "integer_conversion"
        }
      ],
      "type": "array",
      "name": "Record"
    }
  ],
  "content": "<html xmlns=\"http://www.w3.org/1999/xhtml\" xmlns:o=\"urn:schemas-microsoft-com:office:office\" style=\"font-family:arial, 'helvetica neue', helvetica, sans-serif\"> \n<head>\n  <title>Vendor Data Report</title>\n     <style>\n        table {\n            width: 100%;\n            border-collapse: collapse;\n            margin: 20px auto;\n        }\n        th, td {\n            border: 1px solid #ccc;\n            padding: 8px;\n            text-align: left;\n        }\n        th {\n            background-color: #f2f2f2;\n            font-weight: bold;\n        }\n    </style>\n </head> \n <body>\n   <h3>Vendor Report for last 24 hours - </h3>\n    <table>\n        <thead>\n            <tr>\n                <th>Vendor ID </th>\n                <th>UniqueID</th>\n                <th>Vendor Name </th>\n                <th>Operating Unit(s)</th>\n                <th>Site Address </th>\n                <th>No. of Bank</th>\n                <th>Intermediate Bank(s)</th>\n                <th>JRunDt</th>\n            </tr>\n        </thead>\n        <tbody> \n            {{#Record}}\n            <tr>\n                <td>  {{ VendorID }} </td>\n                <td>  {{ RecipeID }} </td>\n\t\t\t\t<td>  {{ VendorName }} </td>\n                <td>  {{ SiteOU }} </td>\n\t\t\t\t<td>  {{ SiteAddress }} </td>\n                <td>  {{ Bank }} </td>\n                <td>  {{ INTBank }} </td>\n                <td>  {{ JobTime }} </td>\n            </tr>\n\t\t\t{{/Record}}\n        </tbody>\n\t</table>\n</body>\n</html>"
}