      {
        "number": 3,
        "provider": "py_eval",
        "name": "invoke_custom_py_code",
        "as": "d3d4871c",
        "keyword": "action",
        "input": {
          "code": "import json\n\ndef process_rows(column_defs, data):\n    columns = [col[\"name\"] for col in column_defs]\n    for i, row in enumerate(data):\n        values = row.get(\"array\", [])\n        if len(values) != len(columns):\n            yield { \"error\": f\"Row {i} length mismatch: {len(values)} vs {len(columns)}\" }\n        else:\n            yield dict(zip(columns, values))\n\ndef main(payload):\n    try:\n        inner_payload = payload.get(\"payload\")\n        if not inner_payload:\n            return { \"error\": \"Missing 'payload' key.\" }\n\n        column_defs = inner_payload.get(\"columnDefinitions\")\n        data = inner_payload.get(\"data\")\n\n        if not column_defs or not data:\n            return { \"error\": \"Missing 'columnDefinitions' or 'data' in payload.\" }\n\n        # Stream rows and build JSON string manually (avoid full list in memory)\n        result_json = \"[\"\n\n        row_gen = process_rows(column_defs, data)\n        first = True\n        for row in row_gen:\n            if not first:\n                result_json += \",\"\n            result_json += json.dumps(row)\n            first = False\n\n        result_json += \"]\"\n\n        return { \"icmquerydata\": result_json }\n\n    except Exception as e:\n        return { \"error\": f\"Internal error occurred: {str(e)}\" }\n",
          "name": "parsedata",
          "code_input": {
            "schema": "[{\"name\":\"payload\",\"type\":\"string\",\"optional\":true,\"label\":\"payload\",\"details\":{\"real_name\":\"payload\"},\"control_type\":\"text\",\"parent\":[\"code_input\",\"data\"]}]",
            "data": {
